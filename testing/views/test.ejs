<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>test</title>
    <link
      rel="stylesheet"
      type="text/css"
      href="https://unpkg.com/trix@2.0.0/dist/trix.css"
    />
    <script
      type="text/javascript"
      src="https://unpkg.com/trix@2.0.0/dist/trix.umd.min.js"
    ></script>
  </head>
  <body>
    <form action="http://localhost:8989/data" method="post" enctype="multipart/form-data">
      <input type="text" name="title" placeholder="title" required />
      <input type="text" name="sub_title" placeholder="sub_title" required />
      <!-- <input type="text" name="blog" placeholder="blog" required /> -->
      <input
        type="file"
        name="display"
        placeholder="image"
        required
      />
      <input id="x" name="content" />
      <trix-editor class="trix-content" input="x"></trix-editor>
      <button type="submit">Sign up</button>
    </form>
    <!-- <div class="trix-content">Stored content here</div> -->
    <script>
        trix-file-accept.preventDefault();
    </script>
  </body>
